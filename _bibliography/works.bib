uri = {\url{papers3://publication/uuid/92E16A52-39F2-4451-B1D3-B2CBC16EA59F}}
}

@article{doi:10.1094/PHYTO-05-19-0168-RVW,
author = {Garrett, Karen A. and Alcala-Briseno, Ricardo and Andersen, Kelsey F and Brawner, Jeremy and Choudhury, Robin and Delaquis, Erik and Fayette, Joubert and Poudel, Ravin and Purves, Duncan and Rothschild, Jennifer and Small, Ian and Thomas-Sharma, Sara and Xing, Yanru},
title = {Effective altruism as an ethical lens on research priorities},
journal = {Phytopathology™},
volume = {0},
number = {ja},
pages = {null},
year = {0},
doi = {10.1094/PHYTO-05-19-0168-RVW}, note ={PMID: 31821114},
URL = {https://doi.org/10.1094/PHYTO-05-19-0168-RVW},
eprint = {https://doi.org/10.1094/PHYTO-05-19-0168-RVW},
abstract = {Effective altruism is an ethical framework for identifying the greatest potential benefits from investments. Here we apply effective altruism concepts to maximize research benefits, in terms of priority stakeholders, pathosystems, and research questions and technologies. Priority stakeholders for research benefits may include smallholder farmers who have not yet attained the minimal standards of the UN Sustainable Development Goals; these farmers would often have the most to gain from better crop disease management, if their management problems are tractable.}
}

